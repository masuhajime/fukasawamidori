        <activity android:name=".IconExitDialogActivity" />
        <activity android:name=".IconAttachAndDettachActivity" />
        <activity android:name=".IconSpaceActivity" />
        <activity android:name=".InterstitialSampleActivity" android:configChanges="orientation|screenSize" />
        <activity android:name=".InterstitialActivity" android:configChanges="orientation|screenSize" />
        <activity android:name=".FinishAdActivity" android:configChanges="orientation|screenSize" />
        
        <meta-data
            android:name="NendDebuggable"
            android:value="true" />
        
    </application>

</manifest>
