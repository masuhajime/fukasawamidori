            </intent-filter>
        </activity>
        <activity android:name=".XmlLayoutActivity" />
        <activity android:name=".JavaCallActivity" />
        <activity android:name=".LayoutSampleActivity" />
        <activity android:name=".SizeSampleActivity" />
        <activity android:name=".SizeActivity" />
        <activity android:name=".TopActivity" />
        <activity android:name=".BottomActivity" />
        <activity android:name=".WebActivity" />
        <activity android:name=".JavaCallLinearActivity" />
        <activity android:name=".JavaCallRelativeActivity" />
        <activity android:name=".JavaCallWithListenerActivity" />
        <activity android:name=".AttachAndDettachActivity" />
        <activity android:name=".ListViewActivity" />
        <activity android:name=".ViewPagerActivity" />
        <activity android:name=".PagerAndListActivity" />
        <activity android:name=".FragmentReplaceActivity" />
        <activity android:name=".DialogSampleActivity" />
        <activity
            android:name=".DialogActivity"
            android:configChanges="orientation|screenSize" />
